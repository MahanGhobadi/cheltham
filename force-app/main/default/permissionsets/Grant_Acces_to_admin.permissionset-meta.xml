<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This PermissionSet enables system administrator to View and perform CRUD (create,read,update and delete) on Book custom object</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Grant Acces to admin</label>
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>true</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>Book__c</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
    <tabSettings>
        <tab>Book__c</tab>
        <visibility>Visible</visibility>
    </tabSettings>
</PermissionSet>
